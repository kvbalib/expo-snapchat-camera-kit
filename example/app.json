{
  "expo": {
    "name": "expo-snapchat-camera-kit-example",
    "slug": "expo-snapchat-camera-kit-example",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.snapchatcamerakit.example"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "expo.modules.snapchatcamerakit.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      ["../app.plugin.js",
        {
          "snapchatKitAppID": "<SNAPCHAT_KIT_APP_ID>",
          "snapchatApiToken": "<SNAPCHAT_API_TOKEN>",
          "snapchatLensGroupID": "<SNAPCHAT_LENS_GROUP_ID>"
        }
      ]
    ]
  }
}
